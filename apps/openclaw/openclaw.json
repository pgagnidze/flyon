{
  "gateway": {
    "mode": "local",
    "bind": "lan",
    "auth": { "mode": "token" },
    "trustedProxies": ["10.89.0.0/16"],
    "controlUi": { "dangerouslyDisableDeviceAuth": true }
  },
  "channels": {
    "slack": {
      "mode": "socket",
      "enabled": true,
      "groupPolicy": "open",
      "dm": { "policy": "open", "allowFrom": ["*"] }
    }
  },
  "agents": {
    "defaults": {
      "memorySearch": {
        "provider": "openai",
        "model": "qwen3-embedding:4b",
        "remote": {
          "baseUrl": "https://ollama.meore.link/v1/",
          "apiKey": "unused"
        }
      }
    }
  },
  "plugins": {
    "entries": {
      "slack": { "enabled": true }
    }
  }
}
